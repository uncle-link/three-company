        script.
            if ($(window).width() <= 960) {
                $(".homevideo #myVideo").remove();
            }
            var bannertimer;
            var circletimer;
            var mouses = true;//是否执行滚动
            function mousefun() {
                mouses = false
                var s = setInterval(function () {
                    mouses = true;
                    clearInterval(s)
                }, 1100)
            }
            function sethomebannersize() {
                $(".home-banner .banneritem,.home-banner").height($(window).height() - $(".header").height());
                $(".home-banner .banneritem .itemtxt .text").css("margin-top", function () {
                    return 0 - $(this).height() / 2;
                });
                if ($(window).width() > 1280) {
                    if ($(".home-service").height() < $(window).height()) {
                        $(".home-service").addClass("height100");
                        $(".home-service").css("height", function () {
                            return $(window).height() - 60;
                        })
                    } else {
                        $(".home-service").removeClass("height100");
                    }
                } else {
                    $(".home-service").removeClass("height100");
                }
            }
            //画圆
            function drawcircle(obj) {
                if (navigator.userAgent.indexOf("MSIE 8.0") > 0) {
                    return;
                }
                obj.find(".text .text-date svg").remove();
                animateEle(obj.find(".text .text-date"), 100)
            }
            function animateEle(objs, baifenbi) {
                //var cwidth=$(".home-banner .banneritem.active .itemtxt .innerbox .text .text-date").width();
                var cwidth = 80;
                if ($(window).width() <= 480) {
                    cwidth = 60;
                }
                objs.each(function () {
                    $(this).svgCircle({
                        parent: $(this)[0],
                        w: cwidth,
                        R: (cwidth - 3) / 2,
                        sW: 3,
                        color: ["#595857", "#595857"],
                        perent: [cwidth - 6, baifenbi],
                        speed: 5000,
                        delay: 0
                    })
                })
            }
            function nextbanner() {
                if (bannertimer == 0) {
                    return;
                }
                var showobj = $(".home-banner .banneritem.active").next();
                if (!showobj.html()) {
                    clearInterval(bannertimer);
                    bannertimer = 0;
                    $("body,html").animate({"scrollTop": [$(window).height() - $(".header").height(), "easeInCubic"]}, 1000);
                    return;
                    //showobj=$(".home-banner .banneritem:first");
                }
                $(".home-banner .banneritem.active").removeClass("active");
                showobj.addClass("active");
                var marginvalue = 0 - $(".home-banner .banneritem.active").index() * $(".home-banner .banneritem").height();
                $(".home-banner .banneritem.active .itemtxt .text").hide();
                $(".home-banner .bannerset").animate({
                    "margin-top": [marginvalue, "easeInCubic"]
                }, 1000, function () {
                    $(".home-banner .banneritem.active .itemtxt .text").removeClass("slideUpRetourn slideDownRetourn magictime").addClass("slideUpRetourn magictime").show();
                });
                drawcircle($(".home-banner .banneritem.active"));

            }
            function prevbanner() {
                if (bannertimer == 0) {
                    return;
                }
                var showobj = $(".home-banner .banneritem.active").prev();
                if (!showobj.html()) {
                    clearInterval(bannertimer);
                    bannertimer = 0;
                    return;
                    //showobj=$(".home-banner .banneritem:last");
                }
                $(".home-banner .banneritem.active").removeClass("active");
                showobj.addClass("active");
                var marginvalue = 0 - $(".home-banner .banneritem.active").index() * $(".home-banner .banneritem").height();
                $(".home-banner .banneritem.active .itemtxt .text").hide();
                $(".home-banner .bannerset").animate({
                    "margin-top": [marginvalue, "easeInCubic"]
                }, 1000, function () {
                    $(".home-banner .banneritem.active .itemtxt .text").removeClass("slideUpRetourn slideDownRetourn magictime").addClass("slideDownRetourn magictime").show();
                });
                drawcircle($(".home-banner .banneritem.active"));
            }
            function tonextbanner() {
                clearInterval(bannertimer);
                if ($(".home-banner .banneritem.active").index() != $(".home-banner .banneritem").size() - 1) {
                    nextbanner();
                    bannertimer = setInterval(function () {
                        nextbanner();
                    }, 5000);
                } else {
                    // $("body,html").animate({ "scrollTop": [$(".home-service").offset().top-$(".header-footer").height()-parseInt($(".home-service .contentitem .btitle").css("margin-top")),"easeInCubic"]},1000);
                    $("body,html").animate({"scrollTop": [$(window).height() - $(".header").height(), "easeInCubic"]}, 1000);
                }
            }
            function toprevbanner() {
                clearInterval(bannertimer);
                if ($(".home-banner .banneritem.active").index() != 0) {
                    prevbanner();
                    bannertimer = setInterval(function () {
                        prevbanner();
                    }, 5000);

                } else {
                    $("body").showbanner({
                        content: "已经是第一个啦"
                    });
                }
            }
            $(document).ready(function () {

                $(".header ul.menu li:eq(7)").addClass("active");
                sethomebannersize();

                $(".home-banner .welcome .more").click(function () {
                    if (!(/msie [6|7|8]/i.test(navigator.userAgent))) {
                        $(".home-banner .welcome").addClass("magictime slideUp");
                    } else {
                        $(".home-banner .welcome").slideUp();

                    }
                    drawcircle($(".home-banner .banneritem.active"));
                    bannertimer = setInterval(function () {
                        nextbanner();
                    }, 5000);
                })
                $(".home-banner").swipe(
                    {
                        swipe: function (event, direction, distance, duration, fingerCount) {

                            if (direction == "up") {
                                if ($(".home-banner .welcome").is(":hidden") || $(".home-banner .welcome").hasClass("slideUp")) {
                                    tonextbanner();
                                } else {
                                    $(".home-banner .welcome .more").click();
                                }

                            } else if (direction == "down") {
                                toprevbanner();
                            }
                        }
                    }
                );

                $(window).resize(function () {
                    sethomebannersize();
                });
                $(".home-banner .oprbox .item.prev").click(function () {
                    toprevbanner();
                });
                $(".home-banner .oprbox .item.next").click(function () {
                    tonextbanner();
                });
                $(".home-banner").mousewheel(function (event, delta, deltaX, deltaY) {
                    //var fromobj=$(event.srcElement);
                    //if(fromobj.parents(".home-banner").html()){
                    if (delta < 0) {//向下
                        if (mouses) {
                            mouses = false;
                            mousefun();
                            if ($(".home-banner .welcome").is(":hidden") || $(".home-banner .welcome").hasClass("slideUp")) {
                                tonextbanner();
                            } else {

                                $(".home-banner .welcome .more").click();
                            }

                        }

                    } else if (delta > 0) {//向上
                        if (mouses) {
                            mouses = false;
                            mousefun();
                            if ($(".home-banner .welcome").is(":hidden") || $(".home-banner .welcome").hasClass("slideUp")) {
                                if ($(document).scrollTop() > 0) {
                                    $("html,body").animate({scrollTop: 0}, 500);
                                } else {
                                    toprevbanner();
                                }
                            }


                        }
                    }
                    return false;
                    //}


                });
